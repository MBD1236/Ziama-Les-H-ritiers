{% extends "admin/base.html.twig" %}
{% block title %}
<div class="row">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);"><i class="ri-home-4-line me-1"></i>Accueil</a></li>
                    <li class="breadcrumb-item active">Ajout</li>
                </ol>
            </div>
            <h4 class="page-title">Enregistrement des bobines</h4>
        </div>
    </div>
</div>
{% endblock title %}
{% block body %}
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h4 class="header-title">Ajout d'une bobine</h4>
            </div>
            <div class="card-body">
                {{form(form)}}
            </div>
        </div>
    </div>
</div>
{% endblock body %}
